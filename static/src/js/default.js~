function hex(s){var m=/rgba?\((\d+), (\d+), (\d+)/.exec(s);if(m){s=(m[1]<<16|m[2]<<8|m[3]).toString(16);while(s.length<6)
s="0"+s;return"#"+s;}
var c=s.split("");if(s.length===4)
return"#"+c[1]+c[1]+c[2]+c[2]+c[3]+c[3];return s;}
