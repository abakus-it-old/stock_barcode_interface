else if(states.action==="stock.products"){self.do_action({type:'ir.actions.client',tag:'stock.products',target:'current',},{clear_breadcrumbs:true,});}
else if(states.action==="stock.menu"){self.do_action({type:'ir.actions.client',tag:'stock.menu',target:'current',},{clear_breadcrumbs:true,});}
else if(states.action==="stock.pickinglist"){self.do_action({type:'ir.actions.client',tag:'stock.pickinglist',target:'current',},{clear_breadcrumbs:true,});}
